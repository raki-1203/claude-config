{
  "$schema": "https://opencode.ai/config.json",
  "model": "anthropic/claude-sonnet-4-5-20250929",
  "plugin": [
    "oh-my-opencode@3.5.2",
    "opencode-antigravity-auth@1.5.1"
  ],
  "agent": {
    "build-error-resolver": {
      "model": "anthropic/claude-opus-4-6"
    },
    "code-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "database-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "doc-updater": {
      "model": "anthropic/claude-opus-4-6"
    },
    "e2e-runner": {
      "model": "anthropic/claude-opus-4-6"
    },
    "python-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "refactor-cleaner": {
      "model": "anthropic/claude-opus-4-6"
    },
    "security-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "tdd-guide": {
      "model": "anthropic/claude-opus-4-6"
    }
  },
  "provider": {
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        }
      }
    },
    "quotio": {
      "models": {
        "gpt-5.3": {
          "name": "Gpt 5.3",
          "options": {
            "reasoning": {
              "effort": "medium"
            }
          },
          "reasoning": true
        },
        "gpt-5.3-codex": {
          "name": "GPT 5.3 Codex",
          "limit": {
            "context": 400000,
            "output": 128000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "reasoning": true,
          "variants": {
            "low": {
              "reasoningEffort": "low",
              "reasoningSummary": "auto",
              "include": [
                "reasoning.encrypted_content"
              ]
            },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "auto",
              "include": [
                "reasoning.encrypted_content"
              ]
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "auto",
              "include": [
                "reasoning.encrypted_content"
              ]
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "reasoningSummary": "auto",
              "include": [
                "reasoning.encrypted_content"
              ]
            }
          }
        }
      },
      "name": "Quotio",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "apiKey": "${QUOTIO_API_KEY}",
        "baseURL": "http://127.0.0.1:8317/v1"
      }
    }
  }
}
