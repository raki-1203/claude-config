  "plugin": [
    "oh-my-opencode@3.5.2",
    "opencode-antigravity-auth@1.5.1"
  ],
  "agent": {
    "build-error-resolver": {
      // "model": "anthropic/claude-opus-4-6"
      // "model": "github-copilot/claude-opus-4.5"
      "model": "quotio/claude-opus-4-6-thinking"
    },
    "code-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "database-reviewer": {
      "model": "quotio/gpt-5.3-codex"
    },
    "doc-updater": {
      // "model": "anthropic/claude-opus-4-6"
      // "model": "github-copilot/claude-opus-4.5"
      "model": "quotio/claude-opus-4-6-thinking"
    },
    "e2e-runner": {
      // "model": "anthropic/claude-opus-4-6"
      // "model": "github-copilot/claude-opus-4.5"
      "model": "quotio/claude-opus-4-6-thinking"
    },
    "python-reviewer": {
